{
  "expo": {
    "name": "HIDA",
    "slug": "HIDA",
    "version": "11.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#f3f7ea"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    
    "ios": {
      "bundleIdentifier": "in.kotcode.hida",
      "buildNumber": "11.0.1",
      "supportsTablet": true
    },
    "android": {
      "package": "in.kotcode.hida",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1101,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f3f7ea"
      }
    },
    "plugins": [
      "@react-native-firebase/app"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "dc8792d3-69a6-4e89-b0f5-f24459804e85"
      }
    }
  }
}
